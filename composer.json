{
    "name": "magadanuhak/steam-api",
    "type": "library",
    "description": "PHP wrapper for the Steam web API",
    "keywords": ["steam", "rust", "api","steam-api"],
    "homepage": "https://github.com/danielmagacf/steam-api-client",
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Tommy Bolger",
            "email": "tommy.bolger@gmail.com",
            "role": "Developer"
        },
        {
             "name": "Daniel Maga",
            "email": "magadanuhak@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Maga\\Steam\\": "src/Steam"
        }
